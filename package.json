{
  "name": "edubot",
  "version": "1.0.0",
  "description": "AI-Powered Educational Assessment Platform",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
